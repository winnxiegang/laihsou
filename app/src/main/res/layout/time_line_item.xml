<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    <View android:layout_width="@dimen/x30"
        android:id="@+id/v_cycle"
        android:layout_height="@dimen/y30"
        android:background="@drawable/cycle_white"/>
    <View android:layout_width="@dimen/x70"
        android:layout_toRightOf="@+id/v_cycle"
        android:layout_height="1dp"
        android:layout_marginTop="@dimen/y15"
        android:id="@+id/v_line"
        android:background="@color/white"/>
    <TextView
        android:id="@+id/tv_date"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/v_cycle"
        android:layout_marginTop="@dimen/y28"
        android:includeFontPadding="false"
        android:text="03.23"
        android:textColor="@color/yellow"
        android:textSize="@dimen/font_10" />
</RelativeLayout>
